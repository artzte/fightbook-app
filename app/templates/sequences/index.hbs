<header>
  <div class="row">
    <div class="columns large-12 small-12 page-title">
      <h2>Sequences</h2>
    </div>
  </div>
</header>
<div class="row sequence-index-row">
  <div class="columns small-12">
    <ul>
      {{#each sequence in content}}
      <li>
        {{#link-to 'sequences.show' sequence.slug}}{{sequence.title}}{{/link-to}}
      </li>
      {{/each}}
    </ul>
  </div>
</div>
